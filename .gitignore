htmlcov/
.tox/
.nox/
.coverage
.coverage.*
.cache
nosetests.xml
coverage.xml
*.cover
.hypothesis/
.pytest_cache/

# IDE / Editor Folders
.idea/
.vscode/
*.swp
*.swo
*~

# OS-specific
.DS_Store
Thumbs.db

# PyTorch / Machine Learning Specific
checkpoint.pt
*.pt
*.pth
wandb/
.ipynb_checkpoints/

# Environment files - generally not versioned if they contain secrets or user-specific paths
# .env
# .venv/
# venv/
# ENV/

